class Solution {
    public int[] twoSum(int[] arr, int target) {
        int[] ans={-1,-1};
        for(int i=0;i<arr.length-1;i++){
            for(int j=i+1;j<arr.length;j++){
                if(target-arr[i]==arr[j]){
                    ans[0]=i;
                    ans[1]=j;
                }
            }
        }
        return ans;
    }
}
